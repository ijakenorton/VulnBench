{
  "_metadata": {
    "description": "Gradient boosting on all datasets for spurious correlation detection",
    "created": "2025-12-10",
    "purpose": "Test if all datasets contain spurious correlations exploitable with bag-of-words + gradient boosting"
  },
  "models": ["gradient_boosting"],
  "datasets": [
    "icvul",
    "mvdsc_mixed",
    "devign",
    "dedup-devign",
    "vuldeepecker",
    "cvefixes",
    "juliet",
    "dedup-big-vul",
    "reveal",
    "diversevul",
    "draper",
    "primevul"
  ],
  "seeds": [
    123456,
    789012,
    345678
  ],
